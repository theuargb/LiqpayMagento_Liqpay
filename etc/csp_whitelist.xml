<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="style-src">
            <values>
                <value id="liqpay-css" type="host">https://static.liqpay.ua</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="liqpay-js" type="host">https://static.liqpay.ua</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="liqpay-api" type="host">https://www.liqpay.ua</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
